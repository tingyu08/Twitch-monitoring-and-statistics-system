{
  "name": "twitch-analytics-backend",
  "version": "0.1.0",
  "main": "dist/app.js",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "npx prisma generate && tsc -p tsconfig.json",
    "start": "node --expose-gc --max-old-space-size=460 dist/server.js",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "test": "cross-env NODE_OPTIONS=--experimental-vm-modules jest",
    "test:watch": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --watch",
    "test:coverage": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --coverage",
    "test:pressure:cache-memory": "cross-env LOG_LEVEL=error ts-node src/scripts/pressure-cache-memory.ts",
    "test:pressure:lifetime-ranking": "cross-env LOG_LEVEL=error ts-node src/scripts/verify-lifetime-ranking-memory.ts",
    "lint:comment-encoding": "ts-node src/scripts/check-comment-encoding.ts",
    "test:perf": "ts-node src/__tests__/performance/message-stats.perf.ts",
    "perf:capture": "ts-node scripts/perf-compare.ts capture",
    "perf:compare": "ts-node scripts/perf-compare.ts compare",
    "db:migrate": "npx prisma migrate dev",
    "db:studio": "npx prisma studio",
    "db:push": "npx prisma db push",
    "db:generate": "npx prisma generate",
    "db:verify:index-cleanup": "ts-node src/scripts/verify-index-cleanup.ts",
    "db:seed": "ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@libsql/client": "^0.15.15",
    "@prisma/adapter-libsql": "^7.1.0",
    "@prisma/client": "^6.19.2",
    "@sentry/node": "^10.32.1",
    "@socket.io/redis-adapter": "^8.3.0",
    "@twurple/api": "^8.0.2",
    "@twurple/auth": "^8.0.2",
    "@twurple/chat": "^8.0.2",
    "@twurple/eventsub-http": "^8.0.2",
    "@types/archiver": "^7.0.0",
    "@types/cookie-parser": "^1.4.10",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.14.10",
    "@types/node-cron": "^3.0.11",
    "@types/pdfkit": "^0.17.4",
    "archiver": "^7.0.1",
    "axios": "^1.7.7",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "effect": "^3.11.9",
    "express": "^4.19.2",
    "express-rate-limit": "^8.2.1",
    "helmet": "^8.1.0",
    "ioredis": "^5.8.1",
    "jsonwebtoken": "^9.0.2",
    "node-cron": "^4.2.1",
    "p-limit": "^3.1.0",
    "pdfkit": "^0.17.2",
    "prisma": "^6.19.2",
    "bullmq": "^5.63.0",
    "rate-limit-redis": "^4.3.0",
    "socket.io": "^4.8.3",
    "typescript": "^5.6.3",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
    "@types/express-rate-limit": "^5.1.3",
    "@types/jest": "^29.5.12",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^8.48.1",
    "@typescript-eslint/parser": "^8.48.1",
    "babel-jest": "^29.7.0",
    "cross-env": "^10.1.0",
    "eslint": "^9.39.1",
    "jest": "^29.7.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.5",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0"
  }
}
